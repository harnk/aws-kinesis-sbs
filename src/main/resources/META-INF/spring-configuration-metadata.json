  ],
  "hints": [
    {
      "name": "aws.kinesis.initial-position-in-stream",
      "values": [
        {
          "value": "LATEST",
          "description": "Start after the most recent data record (fetch new data)."
        },
        {
          "value": "TRIM_HORIZON",
          "description": "Start from the oldest available data record."
        }
      ]
    }
  ]
